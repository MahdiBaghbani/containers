{
  "default": "production",
  "platforms": [
    {
      "name": "production",
      "dockerfile": "services/cernbox-revad/Dockerfile.production",
      "external_images": {
        "build": {
          "name": "golang",
          "build_arg": "BASE_BUILD_IMAGE"
        }
      },
      "dependencies": {
        "common-tools": {
          "service": "common-tools",
          "build_arg": "COMMON_TOOLS_IMAGE"
        },
        "gaia": {
          "service": "gaia",
          "build_arg": "GAIA_IMAGE"
        },
        "revad-base": {
          "service": "revad-base",
          "build_arg": "REVAD_BASE_IMAGE"
        }
      }
    },
    {
      "name": "development",
      "dockerfile": "services/cernbox-revad/Dockerfile.development",
      "external_images": {
        "build": {
          "name": "golang",
          "build_arg": "BASE_BUILD_IMAGE"
        }
      },
      "dependencies": {
        "common-tools": {
          "service": "common-tools",
          "build_arg": "COMMON_TOOLS_IMAGE"
        },
        "gaia": {
          "service": "gaia",
          "build_arg": "GAIA_IMAGE"
        },
        "revad-base": {
          "service": "revad-base",
          "build_arg": "REVAD_BASE_IMAGE"
        }
      }
    }
  ]
}
